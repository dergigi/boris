{
  "version": 2,
  "functions": {
    "api/article-og.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/a/:naddr",
      "destination": "/api/article-og?naddr=:naddr"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

